        "path": "collections/{id}",
        "method": "GET"
      },
      "create": {
        "path": "collections",
        "method": "POST"
      },
      "update": {
        "path": "collections/{id}",
        "method": "PUT"
      },
      "delete": {
        "path": "collections/{id}",
        "method": "DELETE"
      }
    },
    "redirects": {
      "getAllBySite": {
        "path": "redirects/site/{id}",
        "method": "GET"
      },
