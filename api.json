      "getAll": {
        "path": "sites",
        "method": "GET"
      },
      "get": {
        "path": "sites/{id}",
        "method": "GET"
      },
      "create": {
        "path": "sites",
        "method": "POST"
      },
      "update": {
        "path": "sites/{id}",
        "method": "PUT"
      },
      "transfer": {
        "path": "sites/transfer/{id}",
        "method": "PUT"
      },
      "delete": {
        "path": "sites/{id}",
        "method": "DELETE"
      }
    }
