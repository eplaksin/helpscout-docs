    "categories": {
      "getAllByCollection": {
        "path": "collections/{id}/categories",
        "method": "GET"
      },
      "get": {
        "path": "categories/{id}",
        "method": "GET"
      },
      "create": {
        "path": "categories",
        "method": "POST"
      },
      "update": {
        "path": "categories/{id}",
        "method": "PUT"
      },
      "updateOrder": {
        "path": "collections/{id}/categories",
