{
  "domain": "https://docsapi.helpscout.net/",
  "version": "v1",
  "endpoints": {
    "articles": {
      "getAllByCollection": {
        "path": "collections/{id}/articles",
        "method": "GET"
      },
      "getAllByCategory": {
        "path": "categories/{id}/articles",
        "method": "GET"
      },
      "search": {
        "path": "search/articles",
        "method": "GET"
      },
      "getRelated": {
        "path": "articles/{id}/related",
        "method": "GET"
      },
      "getRevisions": {
        "path": "articles/{id}/revisions",
        "method": "GET"
      },
      "get": {
        "path": "articles/{id}",
        "method": "GET"
      },
      "getRevision": {
        "path": "revisions/{id}",
        "method": "GET"
      },
      "create": {
        "path": "articles",
        "method": "POST"
      },
      "update": {
        "path": "articles/{id}",
        "method": "PUT"
      },
      "upload": {
        "path": "articles/upload",
        "method": "POST",
        "contentType": "multipart/form-data"
      },
      "updateViewCount": {
        "path": "articles/{id}/views",
        "method": "PUT"
      },
      "delete": {
        "path": "articles/{id}",
